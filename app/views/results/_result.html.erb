<tr>
  <td><%= result.question.name %></td>
  <td><%= l result.created_at, format: :default %></td>
  <% if result.answer.is_correct %>
    <td>
      <span class="glyphicon glyphicon-ok text-success"></span>
    </td>
  <% else %>
    <td>
      <span class="glyphicon glyphicon-remove text-danger"></span>
    </td>
  <% end %>
</tr>
